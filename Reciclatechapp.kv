<Inicial>:
    FloatLayout:
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size
        Image:
            id: logo_imagem
            source: 'imagem/logo.png'
            size_hint: None, None
            size: 200, 400  
            pos_hint: {'center_x': 0.5, 'center_y': 0.8}
            
        Image:
            id: reciclatech_imagem
            source: 'imagem/reciclatech.png'
            size_hint: None, None
            size: 200, 500  
            pos_hint: {'center_x': 0.5, 'center_y': 0.58}
                                   
        Button:
            text: 'Entrar como coletor'
            background_color: 0, 0, 0, 1
            size_hint: None, None
            size: 180, 50
            radius: [20]
            pos_hint: {'center_x': 0.5, 'center_y': 0.3}
            on_press: root.entrar_como_coletor() 
    
        Button:
            text: 'Entrar como usuário'
            background_color: 0.2, 0.8, 0.2, 1
            size_hint: None, None
            size: 180, 50
            pos_hint: {'center_x': 0.5, 'center_y': 0.2}
            on_press: root.entrar_como_usuario()      

<Login_coletor>:
    FloatLayout:
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size

        BoxLayout:
            orientation: 'vertical'
            spacing: dp(10) 
            size_hint: None, None
            size: dp(300), dp(150) 
            pos_hint: {'center_x': 0.5, 'center_y': 0.5} 

            TextInput:
                id: email_input
                hint_text: 'Email'
                size_hint_x: 1
                size_hint_y: None  
                height: dp(36) 
                background_color: 0.8, 0.8, 0.8, 1
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}

            TextInput:
                id: senha_input
                hint_text: 'Senha'
                password: True
                size_hint_x: 1
                size_hint_y: None  
                height: dp(36) 
                background_color: 0.8, 0.8, 0.8, 1
                pos_hint: {'center_x': 0.5, 'center_y': 0.45}

        Image:
            id: logo_imagem
            source: 'imagem/logo.png'
            size_hint: None, None
            size: 200, 400  
            pos_hint: {'center_x': 0.5, 'center_y': 0.8}

        Label:
            text: '[b]BEM-VINDO(A) COLETOR(A)[/b]'
            markup: True
            color:0,0,0,1
            font_size: 27
            pos_hint: {'center_x': 0.5, 'center_y': 0.6}


        Button:
            text: 'Cadastrar'
            background_color: 0, 0, 0, 1 
            color: 1, 1, 1, 1
            size_hint_x: 0.3
            size_hint_y: 0.06
            pos_hint: {'center_x': 0.5, 'center_y': 0.29}
            on_press: root.cadastrar_coletor()

        Button:
            text: 'Entrar'
            background_color: 0, 1, 0, 1
            color: 0, 0, 0, 1
            size_hint_x: 0.3
            size_hint_y: 0.06
            pos_hint: {'center_x': 0.5, 'center_y': 0.2}
            on_press: root.realizar_login_col()

        Button:
            background_normal: 'imagem/seta.png'
            size_hint: None, None
            size: 32, 20
            pos_hint: {'top': 1, 'left': 1}
            on_release: root.manager.current = 'inicial'
    
